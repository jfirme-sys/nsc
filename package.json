{
  "name": "nodescript-cli",
  "version": "1.0.2",
  "description": "`nscli` is a Node.js CLI tool that allows you to easily run scripts defined in your package.json file. It provides a simple interface for selecting and executing scripts from the terminal.",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.js",
  "bin": {
    "nscli": "dist/index.mjs"
  },
  "repository": {
    "url": "git+https://github.com/jfirme-sys/nscli.git"
  },
  "scripts": {
    "build": "tsup",
    "start": "node ./dist/index.mjs"
  },
  "keywords": [
    "node",
    "cli",
    "command-line",
    "scripts",
    "package.json",
    "npm",
    "yarn",
    "script-runner",
    "interactive",
    "select",
    "execute"
  ],
  "author": "julianofirme",
  "license": "MIT",
  "devDependencies": {
    "@types/inquirer": "^9.0.7",
    "tsup": "^8.0.2",
    "typescript": "^5.4.5"
  },
  "dependencies": {
    "inquirer": "^9.2.20",
    "read-pkg": "^9.0.1"
  }
}
